<body>

	<div data-role="page">
		<div data-role="header" data-position="fixed">
			<h1>新建房间</h1>
		</div>
		<!-- /header -->

		<div data-role="content">
			<ul data-role="listview" data-dividertheme="d" style="margin-top: 0;">
				<li><label for="select-choice-1" class="select">游戏人数</label> <input
					type="range" name="slider" id="select-choice-1" value="8" min="6"
					max="16" /></li>
				<li><img src="assets/img/wolf.png"/><label for="select-choice-2">狼人数</label> <input
					type="range" name="slider" id="select-choice-2" value="2" min="1"
					max="6" /></li>

				<li><label for="slider">先知</label><select name="slider"
					id="slider" data-role="slider">
						<option value="off">关闭</option>
						<option value="on" selected>开启</option>
				</select></li>
				<li><label for="slider">女巫</label><select name="slider"
					id="slider" data-role="slider">
						<option value="off">关闭</option>
						<option value="on" selected>开启</option>
				</select></li>
				<li><label for="slider">猎人</label><select name="slider"
					id="slider" data-role="slider">
						<option value="off">关闭</option>
						<option value="on" selected>开启</option>
				</select></li>
				<li><label for="slider">守卫</label><select name="slider"
					id="slider" data-role="slider">
						<option value="off">关闭</option>
						<option value="on" selected>开启</option>
				</select></li>
				<li><label for="slider">盗贼</label><select name="slider"
					id="slider" data-role="slider">
						<option value="off">关闭</option>
						<option value="on" selected>开启</option>
				</select></li>
				<li><label for="slider">丘比特</label><select name="slider"
					id="slider" data-role="slider">
						<option value="off">关闭</option>
						<option value="on" selected>开启</option>
				</select></li>
				<li><label for="slider">白痴</label><select name="slider"
					id="slider" data-role="slider">
						<option value="off">关闭</option>
						<option value="on" selected>开启</option>
				</select></li>
				<li><label for="slider">普通村民</label><span>人</span></li>
				<li><input type="submit" value="确认开始" /></li>
			</ul>
			<div></div>
		</div>
	</div>
	<!-- /page -->

</body>
</html>